<div class="container">
  <h1><%= @project.name %></h1>
  <%= @project.description %>

  <br/>
  <%# link_to 'Delete', project_path(@project), method: :delete, data: { confirm: "Are you sure you want to delete #{@project.name}?" } %>

  <% @project.photos.uniq.each do |photo| %>
  <div class="col-md-4">
    <div class="img img-responsive">
        <%= image_tag photo.image_url, class: "img img-responsive" %>
    </div>
    <%= link_to 'delete', photo, method: :delete %>
  </div>
  <% end %>
  <%= link_to 'Back', projects_path %>
</div>